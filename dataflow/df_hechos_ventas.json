{
	"name": "df_hechos_ventas",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_tabla_detalle_ventas",
						"type": "DatasetReference"
					},
					"name": "detalleVentas"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_clientes",
						"type": "DatasetReference"
					},
					"name": "dimClientes"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_productos",
						"type": "DatasetReference"
					},
					"name": "dimProductos"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_fechas",
						"type": "DatasetReference"
					},
					"name": "dimFecha"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_gold_hechos_ventas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "cast1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "lookup3"
				},
				{
					"name": "select1"
				},
				{
					"name": "lookup4"
				}
			],
			"scriptLines": [
				"source(output(",
				"          orderNumber as string,",
				"          productKey as string,",
				"          clientId as integer,",
				"          orderDetail as integer,",
				"          shipDetail as integer,",
				"          sales as integer,",
				"          quantity as integer,",
				"          price as decimal(9,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> detalleVentas",
				"source(output(",
				"          clientKey as string,",
				"          firstName as string,",
				"          lastName as string,",
				"          maritalStatus as string,",
				"          genderId as integer,",
				"          birthday as timestamp,",
				"          countryId as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimClientes",
				"source(output(",
				"          productKey as string,",
				"          name as string,",
				"          cost as decimal(9,2),",
				"          line as string,",
				"          categoryID as integer,",
				"          subcategory as string,",
				"          maintenance as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimProductos",
				"source(output(",
				"          DateId as integer,",
				"          Date as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimFecha",
				"detalleVentas, cast1 lookup(clientId == clientKeyCorregida,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"dimClientes derive(clientKeyCorregida = substring(clientKey, 6, 5)) ~> derivedColumn1",
				"dimProductos derive(prodKeyCorrecto = concatWS(\"-\",split(productKey,\"-\")[2],split(productKey,\"-\")[3],split(productKey,\"-\")[4])) ~> derivedColumn2",
				"derivedColumn1 cast(output(",
				"          clientKeyCorregida as integer",
				"     ),",
				"     errors: true) ~> cast1",
				"lookup1, derivedColumn2 lookup(detalleVentas@productKey == prodKeyCorrecto,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2, dimFecha lookup(orderDetail == Date,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup3",
				"lookup3 select(mapColumn(",
				"          orderNumber,",
				"          productKey = detalleVentas@productKey,",
				"          clientId,",
				"          orderDetail,",
				"          shipDetail,",
				"          sales,",
				"          quantity,",
				"          price,",
				"          clientKey,",
				"          firstName,",
				"          lastName,",
				"          maritalStatus,",
				"          genderId,",
				"          birthday,",
				"          countryId,",
				"          clientKeyCorregida,",
				"          productKey = dimProductos@productKey,",
				"          name,",
				"          cost,",
				"          line,",
				"          categoryID,",
				"          subcategory,",
				"          maintenance,",
				"          prodKeyCorrecto,",
				"          orderDetailId = DateId,",
				"          orderDetaildate = Date",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1, dimFecha lookup(shipDetail == Date,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup4",
				"lookup4 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          orderNumber as string,",
				"          productKeyID as string,",
				"          clientKeyID as string,",
				"          orderDetailID as integer,",
				"          shipDetailID as integer,",
				"          sales as integer,",
				"          quantity as integer,",
				"          price as decimal(9,2)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          orderNumber,",
				"          productKeyID = productKey,",
				"          clientKeyID = clientId,",
				"          orderDetailID = orderDetailId,",
				"          shipDetailID = DateId,",
				"          sales,",
				"          quantity,",
				"          price",
				"     )) ~> sink1"
			]
		}
	}
}